class Solution {
public:
    vector<int> minBitwiseArray(vector<int>& nums) {
        vector<int> ans;
        for(int x:nums){
            if(x==2) ans.push_back(-1);
            else{
                int m=1;
                while(x&m) m<<=1;
                ans.push_back(x-(m>>1));
            }
        }
        return ans;
    }
};
